tav:MountainRange rdf:type owl:Class ;
                  rdfs:subClassOf tav:NaturalRegion ;
                  rdfs:comment "A continuous elevated landform having a shared, recognizable geographical origin."@en ;
                  rdfs:label "Mountain Range"@en .


