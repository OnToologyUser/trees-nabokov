#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2004/02/skos/core#Collection
skos:Collection rdf:type owl:Class ;
                rdfs:comment "A meaningful collection of concepts."@en ;
                rdfs:label "Collection"@en .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class ;
             rdfs:comment "A concept is a unit of thought—ideas, meanings, or categories of objects and events."@en ;
             rdfs:label "Concept"@en .


###  http://www.w3.org/2004/02/skos/core#OrderedCollection
skos:OrderedCollection rdf:type owl:Class ;
                       rdfs:subClassOf skos:Collection ;
                       rdfs:comment "An ordered collection of concepts, where both the grouping and the ordering are meaningful."@en ;
                       rdfs:label "Ordered Collection"@en .


###  https://w3id.org/tav#AdministrativeRegion
tav:AdministrativeRegion rdf:type owl:Class ;
                         rdfs:subClassOf tav:Region ;
                         rdfs:comment "A geographical region defined by their administrative boundaries."@en ;
                         rdfs:label "Administrative Region"@en .


###  https://w3id.org/tav#Agent
tav:Agent rdf:type owl:Class ;
          rdfs:comment "A person, organization, or any other artifact that can act or perform some role."@en ;
          rdfs:label "Agent"@en .


###  https://w3id.org/tav#Anthology
tav:Anthology rdf:type owl:Class ;
              rdfs:subClassOf tav:CollectedDocument ;
              rdfs:comment "A collection of selected literary documents having structural or thematic similarities."@en ;
              rdfs:label "Anthology"@en ;
              skos:example "Short Story Collection, American Poetry Collection"@en .


###  https://w3id.org/tav#Article
tav:Article rdf:type owl:Class ;
            rdfs:subClassOf tav:Document ;
            rdfs:comment "A written composition in prose, usually nonfiction, on a specific topic, forming an independent part of a book or other publication, as a newspaper or magazine."@en ;
            rdfs:label "Article"@en .


###  https://w3id.org/tav#Book
tav:Book rdf:type owl:Class ;
         rdfs:subClassOf tav:Document ;
         rdfs:comment "An electronic or printed work of fiction or nonfiction."@en ;
         rdfs:label "Book"@en .


###  https://w3id.org/tav#City
tav:City rdf:type owl:Class ;
         rdfs:subClassOf tav:AdministrativeRegion ;
         rdfs:comment "A local administrative region falling under a municipality."@en ;
         rdfs:label "City"@en .


###  https://w3id.org/tav#CoastalArea
tav:CoastalArea rdf:type owl:Class ;
                rdfs:subClassOf tav:NaturalRegion ;
                rdfs:comment "An area adjacent to a water body, such as an ocean or a sea characterized by unique natural features and ecosystems."@en ;
                rdfs:label "CoastalArea"@en .


###  https://w3id.org/tav#CollectedDocument
tav:CollectedDocument rdf:type owl:Class ;
                      rdfs:subClassOf tav:Document ;
                      rdfs:comment "A collection of Documents or Collections."@en ;
                      rdfs:label "Collected Document"@en .


###  https://w3id.org/tav#Continent
tav:Continent rdf:type owl:Class ;
              rdfs:subClassOf tav:NaturalRegion ;
              rdfs:comment "A continent is type of natural landform comprising a single landmass or as a part of large landmass."@en ;
              rdfs:label "Continent"@en .


###  https://w3id.org/tav#Country
tav:Country rdf:type owl:Class ;
            rdfs:subClassOf tav:AdministrativeRegion ;
            rdfs:comment "A sovereign nation with defined borders and governance."@en ;
            rdfs:label "Country"@en .


###  https://w3id.org/tav#CreativeWork
tav:CreativeWork rdf:type owl:Class ;
                 rdfs:comment "The most generic kind of creative work."@en ;
                 rdfs:label "Creative Work"@en .


###  https://w3id.org/tav#Document
tav:Document rdf:type owl:Class ;
             rdfs:subClassOf tav:CreativeWork ;
             rdfs:comment "A written document of mainly textual character."@en ;
             rdfs:label "Document"@en .


###  https://w3id.org/tav#DocumentSegmentSelector
tav:DocumentSegmentSelector rdf:type owl:Class ;
                            rdfs:subClassOf tav:Selector ;
